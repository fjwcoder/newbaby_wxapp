<view class="big_box">
    <view class="box_item">
        <view class="top_box">
            <view class="item_name">
                姓名：测试
                <text class="item_birthday">生日：2019-01-01</text>
            </view>
            <view class="more_info">更多信息</view>
        </view>
        <view class="bottom_box">
            <view class="item_btn1">删除</view>
            <view class="item_btn2" bindtap="jumpVaccinationList">操作</view>
        </view>
    </view>
    <view class="cu-timeline">
        <!-- <view class="cu-time">全部</view> -->
        <view class="cu-item cur text-{{item}}" wx:for="{{colorArr}}" wx:key="" wx:for-item="item">
            <view class="content bg-{{item}} shadow-blur">
                <view class="">2019-01-01</view>
                <view class="">乙肝疫苗第1剂次</view>
            </view>
        </view>
        <view class="cu-item cur text-grey cuIcon-likefill" bindtap="jumpAddInoculate">
            <view class="content bg-grey shadow-blur">
                <view class="">添加记录</view>
            </view>
        </view>
    </view>
    <view class="padding flex flex-direction">
        <button class="cu-btn bg-green lg">添加记录</button>
    </view>
